view {
	padding: 0;
	margin: 0;
	box-sizing: border-box;
}
@-webkit-keyframes updom {
0% {
		-webkit-transform: translateY(-6rpx);
		        transform: translateY(-6rpx);
}
50% {
		-webkit-transform: translateY(0);
		        transform: translateY(0);
}
100% {
		-webkit-transform: translateY(6rpx);
		        transform: translateY(6rpx);
}
}
@keyframes updom {
0% {
		-webkit-transform: translateY(-6rpx);
		        transform: translateY(-6rpx);
}
50% {
		-webkit-transform: translateY(0);
		        transform: translateY(0);
}
100% {
		-webkit-transform: translateY(6rpx);
		        transform: translateY(6rpx);
}
}
.animation_up {
	-webkit-animation: updom 2s linear .1s infinite alternate;
	        animation: updom 2s linear .1s infinite alternate;
}
